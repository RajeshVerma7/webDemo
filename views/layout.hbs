<body>
   <amp-web-push
    id="amp-web-push"
    layout="nodisplay"
    helper-iframe-url="https://hvhxv.os.tc/amp/helper_frame?appId=a1bace30-0215-43f0-a71e-d1f63e04014d"
    permission-dialog-url="https://hvhxv.os.tc/amp/permission_dialog?appId=a1bace30-0215-43f0-a71e-d1f63e04014d"
    service-worker-url="https://hvhxv.os.tc/OneSignalSDKWorker.js?appId=a1bace30-0215-43f0-a71e-d1f63e04014d"
  ></amp-web-push>

